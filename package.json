{
  "name": "beehive",
  "repository": "git@github.com:ndlib/beehive.git",
  "devDependencies": {
    "browserify": "^6.3.4",
    "browserify-incremental": "^1.5.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.9.0",
    "gulp-watch": "^4.2.4",
    "istanbul": "^0.3.15",
    "jest-cli": "^0.4.12",
    "jquery": "^2.1.4",
    "react": "^0.12.2",
    "react-scroll": "^0.18.0",
    "react-tools": "^0.12.2",
    "reactify": "^0.17.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/jest-cli/bin/jest.js --report lcovonly -- --coverage ./app/assets/javascripts/components/__tests__/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "rootDir": "./app/assets/javascripts/components",
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "jquery"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "preprocessor.js"
    ]
  },
  "dependencies": {
    "coveralls": "^2.11.2"
  }
}
